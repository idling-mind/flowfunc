{
  "name": "flowfunc",
  "version": "0.1.2",
  "description": "Dash wrapper for flume node editor",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components flowfunc -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Najeem Muhammed <najeem@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "flume": "^1.0.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@plotly/dash-component-plugins": "^1.2.3",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.2.5",
    "copyfiles": "^2.1.1",
    "css-loader": "^7.1.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^18.2.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^5.1.6",
    "terser-webpack-plugin": "^5.3.12",
    "webpack": "^5.98.0",
    "webpack-cli": "^4.10.0",
    "webpack-serve": "^4.0.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
